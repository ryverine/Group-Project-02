<div id="Sign-In">
  <div class="container">
    <div class="row">
      <div class="col-8 offset-2">

        <h3><a href="/">Home</a></h3>
        <h3><a href="/locations">All Locations</a></h3>

        <h1>Sign-In</h1>

        <form id="signInForm" class="clearfix mb-4" method="get">
          <!-- this needs to go to user page -->
          <div class="form-group">
            <label for="example-text">Email:</label>
            <input type="email" id="signin-email" class="form-control">
          </div>

        <div class="form-group">
            <label for="example-text">Password:</label>
            <input type="password" id="signin-password" class="form-control">
          </div>

          <div>
          <button id="signin-submit" class="btn btn-primary float-right">Submit</button>
          </div>
        </form>

      </div>
    </div>
  </div>
</div>

